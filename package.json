{
  "name": "shared-nest-library",
  "version": "0.8.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "nest build libraries --tsc",
    "start:dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "**/*.js",
    "notification/**/*",
    "files-crud/**/*",
    "exceptions/**/*",
    "permission/**/*",
    "dm-testing/**/*",
    "schemas/**/*",
    "helpers/**/*",
    "logger/**/*",
    "filter/**/*",
    "models/**/*",
    "user/**/*",
    "crud/**/*",
    "dto/**/*"
  ],
  "dependencies": {
    "@nestjs/common": "7.5.5",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.5.5",
    "@nestjs/microservices": "7.5.5",
    "@nestjs/mongoose": "7.1.2",
    "@nestjs/platform-express": "7.5.5",
    "@nestjs/swagger": "^4.7.8",
    "@ntegral/nestjs-sentry": "1.0.4",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "mongoose": "5.10.1",
    "mongoose-paginate": "5.0.3",
    "mongoose-sequence": "5.3.0",
    "mongoose-timestamp": "0.6.0",
    "postinstall": "0.7.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "swagger-ui-express": "^4.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.3",
    "@nestjs/schematics": "7.2.2",
    "@nestjs/testing": "7.5.5",
    "@types/express": "4.17.3",
    "@types/jest": "26.0.10",
    "@types/mongoose": "5.10.1",
    "@types/mongoose-paginate": "5.0.8",
    "@types/node": "13.13.34",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "6.2.2",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ]
  }
}
